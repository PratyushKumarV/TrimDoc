{
    "manifest_version":3,
    "version": "1.0",
    "name": "TrimDoc",
    "description":"A PDF manipulation chrome extension",
    "permissions": [
        "cookies",
        "storage"
    ],
    "icons":{
            "16":"images/icon16.png",
            "32":"images/icon32.png",
            "48":"images/icon48.png",
            "128":"images/icon128.png",
            "512":"images/icon512.png"
    },
    "action": {
        "default_popup":"index.html",
        "default_icon":{
            "16":"images/icon16.png",
            "32":"images/icon32.png",
            "48":"images/icon48.png",
            "128":"images/icon128.png",
            "512":"images/icon512.png"
        }
    }
}